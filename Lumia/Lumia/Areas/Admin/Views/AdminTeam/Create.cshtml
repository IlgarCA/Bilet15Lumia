@model Employee

@{
    ViewData["Title"] = "Create";
}


<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Create form</h4>
                        <form class="forms-sample" method="post" enctype="multipart/form-data">
                            <div class="form-group">
                                <label asp-for="FullName">FullName</label>
                                <input asp-for="FullName" class="form-control" placeholder="FullName">
                                <span asp-validation-for="FullName" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Professian">Professian</label>
                                <input asp-for="Professian" class="form-control" placeholder="Professian">
                                <span asp-validation-for="Professian" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Bio">Bio</label>
                                <input asp-for="Bio" class="form-control" placeholder="Bio">
                                <span asp-validation-for="Bio" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Twitter">Twitter</label>
                                <input asp-for="Twitter" class="form-control" placeholder="Twitter">
                                <span asp-validation-for="Twitter" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Facebook">Facebook</label>
                                <input asp-for="Facebook" class="form-control" placeholder="Facebook">
                                <span asp-validation-for="Facebook" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Instagram">Instagram</label>
                                <input asp-for="Instagram" class="form-control" placeholder="Instagram">
                                <span asp-validation-for="Instagram" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Linkedin">Linkedin</label>
                                <input asp-for="Linkedin" class="form-control" placeholder="Linkedin">
                                <span asp-validation-for="Linkedin" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="FormFile">FormFile</label>
                                <input asp-for="FormFile" class="form-control" placeholder="FormFile">
                                <span asp-validation-for="FormFile" class="text-text-danger"></span>
                            </div>
                            <button type="submit" class="btn btn-primary me-2">Submit</button>
                            <button class="btn btn-light" asp-action="Index">Cancel</button>
                        </form>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
